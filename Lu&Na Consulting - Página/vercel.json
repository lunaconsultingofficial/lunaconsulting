{
  "buildCommand": "NODE_ENV=production pnpm build",
  "installCommand": "pnpm install --frozen-lockfile --no-optional",
  "framework": "nextjs",
  "functions": {
    "app/**": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}